{
  "expo": {
    "androidStatusBar": {
      "hidden": false,
      "translucent": true,
      "barStyle": "dark-content",
      "backgroundColor": "#fff"
    },
    "name": "Pots",
    "slug": "pots",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "android": {
      "package": "app.pots.com"
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.pots.com"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}